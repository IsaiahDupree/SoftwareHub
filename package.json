    "@supabase/supabase-js": "^2.49.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pdfkit": "^0.17.4",
    "date-fns": "^4.1.0",
    "jsonwebtoken": "^9.0.3",
    "next": "^14.2.0",
    "openai": "^6.16.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
