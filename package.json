{
  "name": "softwarehub",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "npm run port:check && next dev -p 2828",
    "dev:force": "npm run port:kill && next dev -p 2828",
    "build": "next build",
    "start": "next start -p 2828",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "port:check": "bash scripts/port-manager.sh 2828 check",
    "port:kill": "bash scripts/port-manager.sh 2828 kill",
    "port:status": "bash scripts/port-manager.sh 2828 status",
    "port:health": "bash scripts/port-manager.sh 2828 health",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:reset": "supabase db reset",
    "db:migration:new": "supabase migration new",
    "db:push": "supabase db push",
    "db:diff": "supabase db diff",
    "db:studio": "open http://localhost:54323",
    "stripe:listen": "stripe listen --forward-to localhost:2828/api/stripe/webhook",
    "health-check": "npx ts-node scripts/health-check.ts",
    "predev": "npx ts-node scripts/health-check.ts || true"
  },
  "dependencies": {
